local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Chose", HidePremium = true, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Chose",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddButton({
	Name = "Key",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/minecraftakcz360/Script-hub/main/Key"))()
  	end    
})
Tab:AddButton({
	Name = "Permanent Key",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/minecraftakcz360/Script-hub/main/PermKey"))()
  	end    
})
